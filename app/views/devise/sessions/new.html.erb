<link href='https://fonts.googleapis.com/css?family=Averia Serif Libre' rel='stylesheet'>
<div id="course_wrap">

<div id="log_in_shift">

<%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
  <div class="field">
    <%= f.label :email %>:<br />
    <%= f.email_field :email, :size =>"50", autofocus: true %>
  </div>

  <div class="field">
    <%= f.label :password %>:<br />
    <%= f.password_field :password, :size =>"50",  autocomplete: "off" %>
  </div>

  <% if devise_mapping.rememberable? -%>
    <div class="field2">
      <%= f.check_box :remember_me %>
      <%= f.label :remember_me %>
    </div><br>
  <% end -%>

  <div class="actions"><br>
    <%= f.submit "Log in" %><br>
  </div>



<% end %>


<%= render "devise/shared/links" %><br>
</div>
</div>

